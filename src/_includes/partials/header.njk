<header>
  {% include 'logo.svg' %}
  <div class="content">
    <h2>Lea's Blog</h2>
    <p>{{ site.slogan }}</p>
  </div>
  <nav>
    <ul>
      {% for link in nav %}
        <li>
          <a href="{{ site.url }}{{ link.url }}">{{ link.title }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</header>